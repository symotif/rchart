{
	"name": "ehr",
	"version": "0.0.1",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev yarn run dev:all",
		"dev:all": "concurrently -n=svelte,tauri -c=#ff4000,#ffc131 \"yarn run dev:svelte\" \"yarn run dev:tauri\"",
		"dev:svelte": "vite dev",
		"dev:tauri": "tauri dev --config ./vite-tauri-setup.json",
		"tauri": "tauri",
		"build": " vite build",
		"preview": "vite preview",
		"package": "svelte-kit package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.561.0",
		"@lukeed/uuid": "^2.0.0",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/vite": "^4.1.18",
		"@tauri-apps/cli": "^1.5.0",
		"@types/cookie": "^0.6.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"bits-ui": "^2.14.4",
		"concurrently": "^8.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.0",
		"mode-watcher": "^1.1.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-sonner": "^1.0.7",
		"swup": "^4.0.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.18",
		"tslib": "^2.6.0",
		"typescript": "^5.4.0",
		"vite": "^5.0.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@tauri-apps/api": "^1.0.2",
		"clsx": "^2.1.1",
		"cookie": "^0.4.1",
		"d3": "^7.8.5",
		"tailwind-merge": "^3.4.0",
		"tauri-plugin-sql-api": "https://github.com/tauri-apps/tauri-plugin-sql"
	}
}
